{
    "name": "verdigado/piflexformupdate",
    "type": "typo3-cms-extension",
    "description": "Updates data in pi_flexform",
    "authors": [
        {
            "name": "Dirk Wildt",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Verdigado\\Piflexformupdate\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "piflexformupdate"
        }
    }
}
